//these are commands to install zenmap on ubuntu 20.04 [you can easily change this to bash script just add #!/bin/bash\
//also add -y to the end ot apt install
//run script as sudo 

mkdir -p ~/Downloads/zenmap
cd ~/Downloads/zenmap

wget http://archive.ubuntu.com/ubuntu/pool/universe/p/pygtk/python-gtk2_2.24.0-6_amd64.deb
wget http://archive.ubuntu.com/ubuntu/pool/universe/n/nmap/zenmap_7.80+dfsg1-1build1_all.deb

sudo apt install ./*.deb
